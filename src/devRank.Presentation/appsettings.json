{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:G} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:G} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Database": ""
  },
  "Jwt": {
    "Key": "TmFvQWRpYW50YU5hb1NlVGFOb0Rldk1ldUlybWFv",
    "KeyIssuer": "TmF0dXJlQnJhYm8",
    "KeyAudience": "https://github.com/OrganizacaoTesteSum/devRank.api",
    "DiasParaExpiracao": 1
  },
  "AllowedHosts": "*"
}
